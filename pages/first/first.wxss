/* first/first.wxss */
.T{
  width: 100%;
  height: 100vh;
  padding: 0;
  margin:0;
  background-color: rgb(236, 236, 236);
}
.welcome{
  width: 90%;
  height: 60%;
  margin-left: 5%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.welcome image{
  width: 6em;
  height: 5em;
}
.welcome text{
  font-size: 23px;
  font-weight: bold;
  background: linear-gradient(to right, red, blue);        
  -webkit-background-clip: text;        
  color: transparent;
}
.jz{
  display: flex;
  justify-content: center;
  align-items: center;
}