/* pages/rcsc/rcsc.wxss */
.TT{
    /* margin-top: 4em; */
    width: auto;
    height: 100vh;
    background-color: rgb(242, 242, 242);
  }
  .van-search{
    width: 100%;
    position: fixed;
    /* top:10; */
    z-index: 1000;
  }
  .cel{
    position: relative;
    width: 94%;
    height: 100vh;
    top: 4.5vh;
    margin-left: 3%;
  }
  .cell{
    width: 100%;
    height: auto;
    margin-top: 1em;
    background-color: white;
    border-radius: 10px;
  }
  .name{
    display: flex;
    align-items: center;
    width: 100%;
    height: auto;
  }
  .photo{
    width: 10vh;
    height: 10vh;
    border-radius: 10px;
    overflow: hidden;
    margin-left: 3%;
  }
  .pho{
    width: 100%;
    height: 100%;
  }
  .nm{
    width: 42%;
    height: auto;
    margin-left: 2%;
    line-height: 1.5;
    text-overflow: ellipsis;
    overflow: hidden;
  }
  .nm text{
    white-space: nowrap;
  }
  .money{
    width: 30%;
    font-size: 14px;
    margin-left: 3%;
    height: auto;
    text-align: center;
    line-height: 1.8;
    text-overflow: ellipsis;
    overflow: hidden;
  }
  .money text{
    white-space: nowrap;
  }
  .phone{
    display: flex;
    align-items: center;
    width: 100%;
    height: 5vh;
  }

  