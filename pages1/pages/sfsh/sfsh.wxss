/* pages1/sfsh/sfsh.wxss */
.TT{
  width: auto;
  height: auto;
  background-color: rgb(242, 242, 242);
}
.wtms{
  width: 100%;
  height: 20vh;
  background-color: white;
  border-radius: 10px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.wtms image{
  width: 13vh;
  height: 13vh;
  margin-left: 10%;
  border-radius: 50%;
}
.name{
  margin-left: 10%;
  width: 35%;
  overflow: hidden;
}
.nm{
  font-weight: bold;
  font-size: 16px;
}
.sf{
  margin-top: 0.6em;
  font-size: 12px;
}
.bltp{
  width: 100%;
  height: 40vh;
  margin-top: 0.6em;
  background-color: white;
}
.time{
  width: 100%;
  height: 10vh;
  margin-top: 0.6em;
  background-color: white;
  display: flex;
  align-items: center;
}
.tt{
  width: 48%;
  height: auto;
  font-size: 14px;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 15px;
  background-color: rgb(255, 220, 220);
  color: rgb(252, 56, 56);
}
.wb{
  width: 100%;
  height: 12vh;
}
.tp{
  width: 30vh;
  height: 30vh;
  margin-top: 0.6em;
  margin-left: 0.6em;
  background-color: rgb(222, 222, 222);
}
.tp image{
  width: 100%;
  height: 100%;
}
.btn{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  bottom: 4px;
}
.sc{
  width: 46%;
  margin-left: 2%;
  border-radius: 10px;
  background-color: #4fc08d;
}