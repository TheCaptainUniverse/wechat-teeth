.T{
  width: 100%;
  height: auto;
  background-color: rgb(242, 242, 242);
}
.cel{
  width: 96%;
  height: 100vh;
  margin:0 auto;
}
.cell{
  width: 100%;
  height: auto;
  margin-top: 1em;
  background-color: white;
  border-radius: 10px;
}
.name{
  display: flex;
  align-items: center;
  width: 100%;
  height: auto;
}
.photo{
  width: 10vh;
  height: 10vh;
  border-radius: 10px;
  overflow: hidden;
  margin-left: 3%;
}
.pho{
  width: 100%;
  height: 100%;
}
.nm{
  width: 45%;
  height: auto;
  margin-left: 5%;
  line-height: 1.8;
}
.goto{
  width: 22%;
  font-size: 14px;
  height: auto;
  margin-left: 3%;
  margin-top: 10%;
}
.dh{
  width: 100%;
  height: auto;
  display: flex;
}
.dz{
  width: 75%;
  height: 100%;
  margin-left: 3%;
  font-size: 18px;
  display: flex;
}
.img{
  width: 1.5em;
  height: 1.5em;
}
.img image{
  width: 100%;
  height: 100%;
}
.go{
  width: 20%;
  height: 100%;
  margin-left: 2%;
  font-size: 18px;
  display: flex;
}
.movable-area {
  pointer-events: none;
  z-index: 100;
  width: 100%;
  height: calc(100% - 90rpx);
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  box-sizing: border-box;
}

.movable-view {
  pointer-events: auto;
  top: 350rpx;
  height: 100rpx;
  width: 100rpx;
  border-radius: 50%;
  background: #F2F2F2;
  border: 2rpx solid #e6e6e6;
  box-shadow: 0 0 20rpx #dcdcdc;
  display: flex;
  align-items: center;
  justify-content: center;
}
.my1 {
  width: 40rpx;
  height: 40rpx;
}


.tb {
  width: 100%;
  height: 25vh;
  padding-bottom: 10rpx;
  background-color: #B0D8E4;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}
